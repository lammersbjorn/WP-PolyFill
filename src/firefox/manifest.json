{
  "manifest_version": 2,
  "name": "WordPress Translation Filler",
  "version": "3.2.7",
  "description": "Firefox addon to automatically fill translations from Google, DeepL, Microsoft, and OpenAI translation API's. This is the extended version.",
  "resources": [
    "/img/*",
    "wptf-cute-alert.css",
    "error-alert.flac",
    "clipboard-icon.png",
    "warning-marker.png",
    "/_metadata",
    "wptf-options.html",
    "wptf-inject.js",
    "wptf-database.jl",
    "wptf-buildstats.js",
    "wptf-glossary.csv"
  ],
  "background": {
    "scripts": [
      "jsstore.js",
      "jsstore.worker.js"
    ]
  },
  "web_accessible_resources": [
    "img/*",
    "error-alert.flac",
    "wptf-options.html"
  ],
  "options_ui": {
    "page": "wptf-options.html",
    "browser_style": true,
    "open_in_tab": true
  },
  "permissions": [
    "storage",
    "activeTab",
    "tabs",
    "*://api-free.deepl.com/*",
    "*://api.deepl.com/*",
    "*://translation.googleapis.com/*",
    "*://translate.wordpress.org/*",
    "*://api.languagetool.org/*",
    "*://api.languagetoolplus.com/*",
    "*://analytics.languagetoolplus.com/*",
    "*://api.cognitive.microsofttranslator.com/*",
    "*://api.openai.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://translate.wordpress.org/*",
        "https://cdnjs.cloudflare.com/*"
      ],
      "js": [
        "wptf-deeplGloss.js",
        "wptf-spellcheck.js",
        "wptf-buildstats.js",
        "wptf-functions.js",
        "wptf-purify.min.js",
        "wptf-cute-alert.js",
        "wptf-consistency.js",
        "jsstore.js",
        "jsstore.worker.js",
        "wptf-commonTranslate.js",
        "wptf-openAITrans.js",
        "wptf-deeplTrans.js",
        "wptf-googleTrans.js",
        "wptf-microsoftTrans.js",
        "jsdiff.js",
        "wptf-database.js",
        "wptf-inject.js",
        "wptf-importfile.js",
        "contentScript.js"
      ],
      "css": [
        "wptf-cute-alert.css",
        "wptf-style.css"
      ],
      "csv": [
        "wptf-glossary.csv"
      ]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "58.0",
      "id": "{543c6b62-63aa-48b6-a5a0-10e65ce879df}"
    }
  }
}